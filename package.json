{
  "private": true,
  "name": "yad-widget",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:server && npm run build:css && npm run build:worker && npm run build:remix",
    "build:server": "esbuild --platform=node --bundle --format=cjs ./server --outdir=build",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build:css": "tailwindcss -i app/tailwind.css -o app/styles.css --minify",
    "build:remix": "node -r dotenv/config ./node_modules/.bin/remix build",
    "dev": "npm run dev:build & npm run dev:css & npm run dev:server & npm run dev:worker & npm run dev:remix",
    "dev:css": "tailwindcss -i app/tailwind.css -o app/styles.css --watch",
    "dev:server": "node -r dotenv/config ./build/server.js",
    "dev:build": "npm run build:server -- --watch",
    "dev:remix": "node -r dotenv/config ./node_modules/.bin/remix watch",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "postinstall": "patch-package && remix setup node",
    "docker-build-dev": "docker build -t sail-dev -f ./Dockerfile .",
    "docker-run-dev": "docker run --name sail -p 3000:3000 --rm sail-dev",
    "start": "node -r dotenv/config ./build/server.js",
    "lint": "npx eslint \"**/*.{ts,tsx}\"",
    "prettier:check": "prettier --check \"**/*.{ts,tsx}\"",
    "test": "remix setup node && jest",
    "generate-types": "rm -rf app/types/ethers && typechain --target=ethers-v5 'app/abis/*.json' --out-dir app/types/ethers",
    "generate-img-dex-paths": "node ./utils/dex-resolver.js"
  },
  "dependencies": {
    "@esbuild-plugins/node-globals-polyfill": "~0.1.0",
    "@esbuild-plugins/node-modules-polyfill": "~0.1.0",
    "@remix-run/dev": "1.7.2",
    "@remix-run/express": "1.7.2",
    "@remix-run/react": "1.7.2",
    "@remix-run/serve": "1.7.2",
    "@sentry/browser": "~7.14.0",
    "@sentry/node": "~7.14.0",
